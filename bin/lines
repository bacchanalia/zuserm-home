#!/usr/bin/perl
use strict;
use warnings;

sub main(@){
  my $line;
  my $count = 0;
  my $newlines = 0;
  my $stop = 0;
  while($line = <STDIN>){
    if($line eq "\n"){
      $newlines++;
    }else{
      print $line;
      $count++;
      $newlines = 0;
    }
    if($newlines == 3){
      print "\n"x$newlines;
      $stop = 1;
    }
    print "\n" if $count % 10 == 0 and !$stop;
  }
}

&main(@ARGV);
