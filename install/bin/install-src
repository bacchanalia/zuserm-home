#!/usr/bin/perl
use strict;
use warnings;
use lib `dirname $0 | tr -d '\n'`;
use ScriptScript;

sub main(@) {
    installFromGit "git://github.com/teleshoes/klomp.git", ''
      . " ./dependency-install.pl;"
      . " ./install.pl;"
      ;

    installFromGit "git://github.com/teleshoes/qtbigtext.git";

    installFromGit "git://github.com/teleshoes/twip.git";

    installFromGit "git://github.com/bacchanalia/KitchenSink.git";

    installFromGit "git://github.com/aktau/hhpc.git", ''
      . " make;"
      . " sudo cp hhpc /usr/local/bin;"
      ;
}

&main(@ARGV);
